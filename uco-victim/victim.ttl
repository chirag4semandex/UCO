# baseURI: https://unifiedcyberontology.org/ontology/uco/victim
# imports: https://archive.unifiedcyberontology.org/ontology/uco/core/0.4.1
# imports: https://archive.unifiedcyberontology.org/ontology/uco/role/0.4.1

@base <https://unifiedcyberontology.org/ontology/uco/victim> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix role: <https://archive.unifiedcyberontology.org/ontology/uco/role/0.4.1#> .
@prefix victim: <https://archive.unifiedcyberontology.org/ontology/uco/victim/0.4.1#> .
@prefix xs: <http://www.w3.org/2001/XMLSchema#> .

<https://unifiedcyberontology.org/ontology/uco/victim>
	a owl:Ontology ;
	rdfs:label "uco-victim"@en ;
	owl:imports
		<https://archive.unifiedcyberontology.org/ontology/uco/core/0.4.1> ,
		<https://archive.unifiedcyberontology.org/ontology/uco/role/0.4.1>
		;
	owl:ontologyIRI <https://unifiedcyberontology.org/ontology/uco/victim> ;
	owl:versionIRI <https://archive.unifiedcyberontology.org/ontology/uco/victim/0.4.1> ;
	owl:versionInfo "0.4.1" ;
	.

victim:Victim
	a owl:Class ;
	rdfs:subClassOf role:NeutralRole ;
	rdfs:label "Victim"@en ;
	rdfs:comment "Person or organization that is the target of some malicious action."@en ;
	.

victim:VictimTargeting
	a owl:Class ;
	rdfs:subClassOf victim:Victim ;
	rdfs:label "VictimTargeting"@en ;
	rdfs:comment "Characteristics of people or organizations that are the target of some malicious activity."@en ;
	.

